        "type": {
          "dataType": "string",
          "isMultiple": false
        },
        "label": "vCenter",
        "id": "vCenter",
        "constraints": {
          "required": true
        },
        "valueList": {
          "type": "scriptAction",
          "id": "com.clouddepth.datastore_cluster_management.integration-service-1.actions/getvCenters",
          "parameters": []
        }
      },
      "datastoreClusterName": {
        "type": {
          "dataType": "string",
          "isMultiple": false
        },
        "label": "Datastore Clusters",
        "id": "datastoreClusterName",
        "constraints": {
          "required": true
        },
        "valueList": {
          "type": "scriptAction",
          "id": "com.clouddepth.datastore_cluster_management.integration-service-1.actions/getDatastoreClusters",
          "parameters": [
            {
              "vCenter": "vCenter",
              "$type": {
                "dataType": "string"
              }
            }
          ]
        }
      },
      "defaultVmBehavior": {
        "type": {
          "dataType": "string",
          "isMultiple": false
        },
        "label": "Default VM Behavior",
        "id": "defaultVmBehavior",
        "constraints": {},
        "valueList": [
          {
            "value": "partiallyAutomated",
            "label": "Partially Automated"
          },
          {
            "value": "manual",
            "label": "Manual"
          },
          {
            "value": "automated",
            "label": "Fully Automated"
          }
        ],
        "default": "automated"
      },
      "ioLoadImbalanceThreshold": {
        "type": {
          "dataType": "decimal",
          "isMultiple": false
        },
        "label": "I/O Load Imbalance Threshold (%)",
        "id": "ioLoadImbalanceThreshold",
        "constraints": {
          "min-value": 1,
          "max-value": 100
        },
        "default": 50,
        "step": 1,
        "signpost": "1% - Aggressive\n100% - Conservative"
      },
      "ioLatencyThreshold": {
        "type": {
          "dataType": "decimal",
          "isMultiple": false
        },
        "label": "I/O Latency Threshold (ms)",
        "id": "ioLatencyThreshold",
        "constraints": {
          "min-value": 5,
          "max-value": 100
        },
        "step": 1,
        "default": 15,
        "signpost": "Allowed value is between 5-100 ms"
      },
      "minSpaceUtilizationDifference": {
        "type": {
          "dataType": "decimal",
          "isMultiple": false
        },
        "label": "Min Space Utilization Difference (%)",
        "id": "minSpaceUtilizationDifference",
        "constraints": {
          "min-value": 1,
          "max-value": 50
        },
        "default": 5,
        "step": 1,
        "signpost": "Allowed value can be in a range between 1-50%"
      },
      "spaceThresholdMode": {
        "type": {
          "dataType": "string",
          "isMultiple": false
        },
        "label": "Space Threshold Mode",
        "id": "spaceThresholdMode",
        "constraints": {},
        "valueList": [
          {
            "value": "freeSpace",
            "label": "Free Space"
          },
          {
            "value": "utilization",
            "label": "Utilization"
          }
        ],
        "default": "utilization"
      },
      "freeSpaceThresholdGB": {
        "type": {
          "dataType": "decimal",
          "isMultiple": false
        },
        "label": "Free Space Threshold (GB)",
        "id": "freeSpaceThresholdGB",
        "constraints": {
          "min-value": 1
        },
        "step": 1,
        "default": 1
      },
      "spaceUtilizationThreshold": {
        "type": {
          "dataType": "decimal",
          "isMultiple": false
        },
        "label": "Space Utilization Threshold (%)",
        "id": "spaceUtilizationThreshold",
        "constraints": {
          "min-value": 50,
          "max-value": 100
        },
        "step": 1,
        "default": 80,
        "signpost": "Allowed value can be in a range between 50-100%"
      },
      "ioLoadBalanceEnabled": {
        "type": {
          "dataType": "boolean"
        },
        "label": "I/O Load Balance Enabled",
        "default": true
      },
      "defaultIntraVmAffinity": {
        "type": {
          "dataType": "boolean"
        },
        "label": "Default Intra VM Affinity",
        "default": true
      },
      "loadBalanceInterval": {
        "type": {
          "dataType": "decimal"
        },
        "label": "Load Balance Interval (mins)",
